  </ItemGroup>
  <ItemGroup>
    <None Remove="wwwroot\**" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\LinqToDB.Identity\src\LinqToDB.Identity\LinqToDB.Identity.csproj" />
